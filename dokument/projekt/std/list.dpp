class Stack {
    int[] arr;
    int size;


    Stack() {};

    int at(int pos) {
        return this.arr[pos];
    };

    void push(int val) {
        this.arr = [val] + this.arr;
        this.size = this.size + 1;
    };

    int pop() {
        this.size = this.size - 1;
        int res = this.arr[1];
        this.arr = this.arr - [res];
        return res;
    };
}